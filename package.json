{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "generate-sw": "workbox injectManifest workbox-config.js",
        "build-thai-address": "cd nova-components/ThaiAddress && npm run dev",
        "build-thai-address-prod": "cd nova-components/ThaiAddress && npm run prod",
        "build-orderstatus": "cd nova-components/Orderstatus && npm run dev",
        "build-orderstatus-prod": "cd nova-components/Orderstatus && npm run prod",
        "build-checkers": "cd nova-components/Checkers && npm run dev",
        "build-checkers-prod": "cd nova-components/Checkers && npm run prod",
        "build-qr-code-scan": "cd nova-components/QrCodeScan && npm run dev",
        "build-qr-code-scan-prod": "cd nova-components/QrCodeScan && npm run prod",
        "build-order-print": "cd nova-components/OrderPrint && npm run dev",
        "build-order-print-prod": "cd nova-components/OrderPrint && npm run prod"
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "axios": "^0.19",
        "cross-env": "^7.0",
        "gulp-autoprefixer": "^7.0.1",
        "jquery": "^3.5.1",
        "laravel-mix": "^5.0.1",
        "laravel-mix-purgecss": "^5.0.0",
        "lodash": "^4.17.19",
        "popper.js": "^1.12",
        "resolve-url-loader": "^3.1.0",
        "tailwindcss-plugins": "^0.3.0",
        "sass": "^1.28.0",
        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "element-ui": "^2.13.2",
        "google-maps-api-loader": "^1.1.1",
        "laravel-echo": "^1.8.0",
        "postcss-cli": "^7.1.1",
        "postcss-import": "^12.0.1",
        "pusher-js": "^6.0.3",
        "tailwindcss": "^1.9.6",
        "vue": "^2.6.12",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2",
        "vue-router": "^3.3.4",
        "vue-thailand-address": "^3.3.0",
        "vue2-google-maps": "^0.10.7",
        "vuex": "^3.4.0"
    }
}
